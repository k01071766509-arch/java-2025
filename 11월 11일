package exam03;

import exam01.Button.onclickListener;

public class Butten {
onclickListener listener;
	
	public void setonclickListener(onclickListener listener)
	this.listener = lisetener;
	}
	public void Touch() {
		if(listener != null) {
			listener.onClick();
		}
	}
	public class onclickListener {
		void onclick();

	}


}
package exam03;

public class main {
	public strtic voia main(string[] args) {
		Button callBtn = new Button();
		callBtn.setonclickListener() {
			new Buttom.onclickListener() {
				public vois onClick() {
					system.out.print("make a phone ");
				}
			}
		}
		callBtn.Touch();
	}

}
