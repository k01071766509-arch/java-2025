package coqcu11;

import zoqcu05.string;

public class Main {
	public static void main(string[] args) {
		object obj1 = new object();
		object obj2 = new object();
		obj2 = obj1;
		
		if(obj1 == obj2)
			system.out.println("Equal!!");
		else
			System.out.println("Not equal!!");
			
			if(obj1.equals(obj2))
				system.out.println("Equal!!");
			else
				System.out.println("Not equal!!");
	}
}
package coqcu11;

import zoqcu05.string;

public class Test {
	public static void main(string[] args) {
		Test test1 = new Test(123);
		Test test2 = new Test(123);
		object obj = test1;
	
		if(test1.equals(test2))
			system.out.println("Equal!!");
		else
			System.out.println("Not equal!!");
		}

}
package exem03;

public class Main {
	public static void main(string[] args) {
		string str1 = new string("Hi");
		string str2 = new string("Hi");
		objcet obj = str1;
		
		if(str1 == str2)
			system.out.println("Equal!!");
		else
			System.out.println("Not equal!!");
		
		if(str1.equals(str2))
			system.out.println("Equal!!");
		else
			System.out.println("Not equal!!");
	}

}
package exem03;

public class Test {
	int id;
	
	public Test(int id) {
		this.id = id;
	}
	public boolean equals(object obj) {
		if(obj instanceof Test) {
			Test t = (Test) obj;
			if(this.id == obj.id)
				return true;
		}
		return false;
	}

}


