	}

}
package exam;

public class Dragon extends Enemy {
	int power;
	int reward;
	int winRatio;
	
	Dragon(int power, int reward){
		this.power = power;
		this.reward = reward;
		this.winRatio = winRatio;
	}
	
	void Attack(player player) {
		Random rand = new Random();
		int val = rand.nextInt(100);
		
		if(val < 50) {
			system.out,println("Dragcn win!!");
			Damage(player);
		}
		
	}

}
		else {
			system.out,println("Dragcn win!!");
			Damage(player);
		}
		}
