package chap02;
public class RunStatementExaple {
	public static void main(String[] args) {
		int x = 1;
		int y =2;
		int result = x+ y;
		System.out.println(result);
	}
}
package chap03;

public class exam02 {
	public static void main(String[] args) {
		int hour = 3;
		int minute = 5;
		System.out.println(hour + "시간" + minute+ "분");
		
		int totalMinute = (hour*60) +minute;
		System.out.println("총" + totalMinute + "분");
	}

}
