package chap03;

public class exam05 {
	public static void main(String[] args) {
		int x =3;
		int y=5;
		System.out.println("x:" +x+ ",y:"+y);
		
		int temp = x;
		x = y;
		y=temp;
		System.out.println("x:"+x+",y:"+y);
	}
package chap03;

public class exam06 {
	public static void main(String[] args) {
		int hour = 3;
		int minute =5;
		System.println(hour + "시간"+"minute"+"분");
		
		int totalMinute = (hour*60) + minute;
		System.out.println("총"+totalMinute+ "분");
	}

}


