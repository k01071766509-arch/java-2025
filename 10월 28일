package eaan;

public class cat extends Animl {
		static int normalHeight = 2;
		int jumpHeight = jumpHeigh;
		
		cat(int jumpHeight) {
			this.jumpHeight = jumpHeight;
			
		}
		boolean InHealth() {
			if(jumpHeight > noramlHeight) {
				return true;
			}
			void Heal() {
				if(InHealth()) {
						System.out.println("");
				}
			}
		}

	}


}
