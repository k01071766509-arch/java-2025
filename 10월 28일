package sec4;

public class Dog {static int noramlspeea
	int runspeed;
	
	boolean  IsHealthy() {
		if(runspeed > normalspeed)
		return true;
		else
			return false;
	}void Heal() {
		if(IsHealthy()) {
			System.out.println("No problem");
		}
		else {
			system.out.print("Run more");
		}
	}

}
package sec4;

public class cat {
	static int normalHeight = 2;
	int jumpHeight = jumpHeigh;
	
	cat(int jumpHeight) {
		this.jumpHeight = jumpHeight;
		
	}
	boolean InHealth() {
		if(jumpHeight > noramlHeight) {
			return true;
		}
		void Heal() {
			if(InHealth()) {
					System.out.println
			}
		}
	}

}
