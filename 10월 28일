package sec4;

public class Acc {
	int bal;
	
	Acc(int bal){
		this.bal = bal;
	}
	void showBal ()
	System.out.print("sav  bal :" + bal);

}
package sec4;

public class sav extends Acc {
	int bal;
	
	sav(int bal) {
		super(bal);

	}

}
